{
  "name": "Markdown",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Automatically interprets the clothing you are viewing and presents you with alternatives from other online clothing stores.",
  "author": "Jackson Lawrence",
  "icons": {
    "128": "icons/display-logo.png"
  },
  "permissions": [
    "tabs",
    "*://*/*",
    "webNavigation",
    "storage"
  ],
  "background": {
    "page": "src/background.html"
  },
  "browser_action": {
    "default_icon": {
      "128": "icons/markdown-logo128.png"
    },
    "default_title": "Markdown",
    "default_popup": "/src/popup.html"
  },
  "web_accessible_resources": [
    "src/keywords.json",
    "src/fonts/Poppins-Bold.ttf",
    "src/fonts/Poppins-Regular.ttf",
    "src/stores.json"
  ],
  "content_security_policy": "script-src 'self' https://browser.sentry-cdn.com/5.12.0/bundle.min.js https://cdn.mxpnl.com/libs/mixpanel-2-latest.min.js; object-src 'self'"
}
